{
  "name": "@action-atlas/ai",
  "version": "0.1.0",
  "private": true,
  "description": "AI services (embeddings, vector search) for Action Atlas",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate-embeddings": "tsx src/scripts/generate-embeddings.ts"
  },
  "dependencies": {
    "@action-atlas/types": "workspace:*",
    "@ai-sdk/openai": "^1.0.0",
    "ai": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2"
  }
}
